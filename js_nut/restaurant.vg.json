{
    "$schema": "https://vega.github.io/schema/vega-lite/v5.json",
    "background": "#f3f4d1",
    "width": 650,
    "height": 350,
    "title": "Number of Subway Restaurants Overtime",
    "data": {
      "url": "js_nut/restaurants.csv"
    },
    "layer":[
        {
            "mark": {"type":"bar","width":{"band": 2.0}},
            "encoding": {
            "x": {"field": "Year", "type": "ordinal"}, 
            "y": {"field": "Number of Subway", "type": "quantitative"},
            "size":{"value":30},
            "color": {

                "field": "Year",
                "type": "ordinal",
                "legend":null,
                "scale": {
                    "range": ["#023020"]
                }
            }
            }
            
        }
        ,
        {
            "mark": {
                "type": "text",
                "align": "center",
                "baseline": "line-top",
                "color":"black",
                "dy":-15
            },
    
            "encoding": {
                "x": {"field": "Year", "type": "ordinal"},
                "y": {"field": "Number of Subway", "type": "quantitative"},
                "text": {"field": "Number of Subway", "type": "quantitative"}

            }
        }

    ]

   }
    
    


  
